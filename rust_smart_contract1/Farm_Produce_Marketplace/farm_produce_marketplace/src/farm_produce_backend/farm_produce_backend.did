type Product = record {
  id : nat64;
  name : text;
  description : text;
  seller : principal;
  quantity : nat64;
  price : nat;
};
service : () -> {
  create_product : (text, text, nat64, nat) -> ();
  get_products : (text) -> (vec Product) query;
  update_product : (nat64, nat64, text, nat) -> ();
}
